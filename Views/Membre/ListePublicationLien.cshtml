@model ICollection<NetAtlas_The_True_Project.Models.Lien>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "Publication image/vidéo";
    Layout = "~/Views/Shared/_LayoutMembre.cshtml";
    ViewBag.UserNameRecup = @ViewBag.UserNameRecup;

}
<div class="wrapper">
    <div class="page-header">
      <div class="page-header-image"></div>
      <div class="content">
        <div class="container">
          <div class="row">
            <div class="col-lg-5 col-md-6 offset-lg-0 offset-md-3">
              <div id="square7" class="square square-7"></div>
              <div id="square8" class="square square-8"></div>
               @foreach(var item in Model){
                  <div class="card card-register card-center">
                    <div class="card-header">
                      <h3 class="text-info" style="margin:10px">@@Moi</h3>
                    </div>
                    <div class="card-body">
                        <a href="https://@item.AdresseUrl">@item.AdresseUrl</a>
                        <div class="card-footer">
                      <h4>Un lien :)</h4>
            <a asp-action="SupprimerPublication" asp-route-id="@item.IdRessource" class="btn-danger btn">Retirer</a>
                    </div>
                    </div>
                  </div>
               }
            </div>
          </div>
          <div class="register-bg"></div>
          <div id="square1" class="square square-1"></div>
          <div id="square2" class="square square-2"></div>
          <div id="square3" class="square square-3"></div>
          <div id="square4" class="square square-4"></div>
          <div id="square5" class="square square-5"></div>
          <div id="square6" class="square square-6"></div>
        </div>
      </div>
    </div>
  </div>